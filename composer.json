{
    "name": "gzhegow/reflection",
    "description": "Reflection Library",
    "type": "library",
    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/6562680/support"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        
        "psr/simple-cache": "^1.0",
        
        "gzhegow/support": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Gzhegow\\Reflection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gzhegow\\Reflection\\Tests\\": "tests/src"
        }
    }
}